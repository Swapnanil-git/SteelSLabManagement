@model SteelSlabManagement.Models.Production

<h2>Production Details</h2>

<div class="form-group">
    <label>Production ID:</label>
    <p>@Model.ProductionId</p>
</div>
<div class="form-group">
    <label>Order ID:</label>
    <p>@Model.OrderId</p>
</div>
<div class="form-group">
    <label>Stage:</label>
    <p>@Model.Stage</p>
</div>
<div class="form-group">
    <label>Timestamp:</label>
    <p>@Model.Timestamp</p>
</div>

<a asp-action="Edit" asp-route-id="@Model.ProductionId" class="btn btn-primary">Edit</a>
<a asp-action="Index" class="btn btn-secondary">Back to List</a>
